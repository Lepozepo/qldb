    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "dotenv": "^8.0.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.6.4",
